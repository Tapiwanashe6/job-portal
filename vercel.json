{
  "version": 2,
  "builds": [
    {
      "src": "server/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api(?:/.*)?",
      "dest": "/server/api/index.js"
    },
    {
      "src": "^/$",
      "dest": "/server/public/index.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "/server/public/$1"
    }
  ]
}
